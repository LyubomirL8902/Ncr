<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/mvc
        http://www.springframework.org/schema/mvc/spring-mvc.xsd"> 
        

		<bean id="sum" class="com.LL.springApp.beans.Sum"></bean>
		<bean id="avg" class="com.LL.springApp.beans.Average"></bean>	   
		<bean id="min" class="com.LL.springApp.beans.Minimum"></bean>
		<bean id="max" class="com.LL.springApp.beans.Maximum"></bean>			
	
</beans> 